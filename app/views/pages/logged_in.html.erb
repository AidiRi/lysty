<%= render 'welcome' %>

<%= "Welcome back #{@user.name}" %>

<h3>My Lists</h3>

  <% @lists.each do |list|%>
    <%= link_to list.title, list %>
    <ol>
    <% list.products.each do |product| %>
        <li><%= link_to product.name, product %></li>
    <% end %>
    </ol>
  <% end %>

<%= @no_lists %>
